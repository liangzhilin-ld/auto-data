    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        CNN_ID, CNN_NAME, DB_TYPE, DB_NAME, CNN_HOST, CNN_PORT, DRIVER_CLASS, USERNAME, PASSWORD, REMARKS, OPERATION, CREATE_TIME, UPDATE_TIME
